    "build:webpack": "rimraf bundle && webpack",
    "start": "webpack-dev-server --progress",
    "predeploy": "yarn run build:webpack && cpx ./static/**/* ./bundle/assets && cpx ./static/**/.* ./bundle/assets && yarn run hackRoutes",
